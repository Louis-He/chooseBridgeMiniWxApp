/* pages/university/specificUni/specificUni.wxss */
@import '../weui.wxss';
page {
  background: linear-gradient(#1C5388, #0F3881);
}
.weui-search-bar {
  background: linear-gradient(#1C5388, #0F3881);
  border-color: transparent;
}
.weui-search-bar::after {
  border: none;
}
.weui-search-bar__form {
  border-radius: 100rpx;
}
.weui-search-bar__label {
  border-radius: 100rpx;
}
 .searchbar-result{
    margin-top: 0;
    font-size: 30rpx;
}
.searchbar-result:before{
    display: none;
}
.weui-cell{
    padding: 12px 15px 12px 35px;
}
.cancel{
  color: white;  
  font-size: 28rpx;
} 