<!--pages/user/privilegeForm/errorEmail.wxml-->
<view class="page">
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
            <icon type="warn" size="93" color='white'></icon>
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">访问失败</view>
            <view class="weui-msg__desc">请先绑定您的高校邮箱，然后再重试</view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
                <button class="weui-btn-recommand" bindtap='bindEmail'>前往绑定</button>
                <button class="weui-btn" bindtap='cancel'>返回</button>
            </view>
        </view>
        <view class="weui-msg__extra-area">
            <view class="weui-footer">
                <view class="weui-footer__text">Copyright © 2018 桥选校园</view>
            </view>
        </view>
    </view>
</view>